<?xml version="1.0" encoding="UTF-8"?>
<params>
	<db_name>alignDatabase.db</db_name>	
	<photo_table>
			<name>photos</name>
			<full_path_col>fullPath</full_path_col>
			<reviewed_col>isReviewed</reviewed_col>
			<action_taken_col>actionTaken</action_taken_col>
	</photo_table>
	<param_table>
		<name>params</name>
		<name_col>parameter_name</name_col>
		<val_col>parameter_val</val_col>
	</param_table>
	<roots_table>
		<name>roots</name>
		<root_col>root_paths</root_col>
	</roots_table>
	<possible_actions>
		<action_none>none</action_none>
		<action_clockwise>r_right_cw</action_clockwise>
		<action_ccw>r_left_ccw</action_ccw>
		<action_180>r_180</action_180>
		<action_delete>delete</action_delete>
	</possible_actions>
</params>